{
  "name": "antidote_ts_client_test",
  "version": "0.0.1",
  "description": "An experimental Antidote client for nodejs, written in typescript",
  "main": "./dist/antidote.js",
  "typings": "./dist/antidote",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "protobufjs": "5.0.1"
  },
  "devDependencies": {
    "proto2ts": "2.0.0",
    "rimraf": "^2.5.4",
    "typescript": "^2.0.3"
  },
  "scripts": {
    "compile": "tsc",
    "postinstall": "typings install",
