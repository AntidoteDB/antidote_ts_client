{
  "name": "antidote_ts_client",
  "version": "0.1.2",
  "description": "An experimental Antidote client for nodejs, written in typescript",
  "homepage": "https://github.com/syncfree/antidote_ts_client",
  "repository": {
    "type": "git",
    "url": "https://github.com/syncfree/antidote_ts_client.git"
  },
  "main": "./dist/antidote.js",
  "typings": "./dist/antidote",
  "author": "Peter Zeller <p_zeller@cs.uni-kl.de>",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/mocha": "^2.2.32",
    "@types/msgpack-lite": "^0.1.1",
    "@types/node": "^6.0.45",
    "@types/protobufjs": "^5.0.30",
    "msgpack-lite": "^0.1.20",
    "protobufjs": "5.0.1"
  },
  "devDependencies": {
    "mocha": "^3.1.2",
    "proto2ts": "git://github.com/SINTEF-9012/Proto2TypeScript.git#d96cfd3dc7afcdbea86114cefca1a761fe0d59ca",
    "rimraf": "^2.5.4",
    "typescript": "^2.0.3"
  },
  "scripts": {
    "compile": "tsc",
    "pretest": "npm run compile",
    "test": "mocha -R spec \"dist/**/*test.js\"",
    "clean": "rimraf dist/*",
    "watch": "tsc --watch",
    "lint": "tslint --project tsconfig.json",
    "prepublish": "npm run compile",
    "typedoc": "typedoc --mode modules --out docs/ src/antidote.ts --tsconfig tsconfig.json --readme manual.md && touch docs/.nojekyll"
  }
}
